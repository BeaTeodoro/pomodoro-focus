{
  "version": 2,
  "builds": [
    {
      "src": "api/token.js",
      "use": "@vercel/node"
    },
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/token",
      "dest": "/api/token.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}